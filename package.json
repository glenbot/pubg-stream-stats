{
  "name": "pubg-stream-stats",
  "version": "0.2.0",
  "description": "Get PUBG API data to text files for use in streaming software",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build_windows": "./node_modules/.bin/electron-builder --win"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glenbot/pubg-stream-stats.git"
  },
  "keywords": [
    "pubg",
    "stream",
    "API",
    "stats",
    "OBS"
  ],
  "upgradeCode": "772ccad8-2f6f-4ae8-85f6-0e1c36665f14",
  "build": {
    "appId": "com.devnull.pubstreamstats",
    "msi": {
      "perMachine": true
    },
    "win": {
      "target": "msi"
    },
    "electronVersion": "2.0.6",
    "linux": {
      "target": "deb",
      "synopsis": "PUBG Stream Stats"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true
    }
  },
  "author": "Glen Zangirolami",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glenbot/pubg-stream-stats/issues"
  },
  "homepage": "https://github.com/glenbot/pubg-stream-stats#readme",
  "devDependencies": {
    "electron": "^2.0.6",
    "electron-builder": "^20.26.1"
  },
  "dependencies": {
    "battlegrounds": "^1.5.1",
    "electron-localshortcut": "^3.1.0",
    "electron-settings": "^3.2.0",
    "sprintf-js": "^1.1.1"
  }
}
